{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"]
    },
      "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,

    "moduleResolution": "bundler",
    "target": "esnext",
    "allowSyntheticDefaultImports": true
  },
  "include": ["src", "App.tsx"]
}
